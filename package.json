{
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build --no-cache --continue --concurrency 11",
    "dev": "dotenv -- turbo run dev --no-cache --continue --concurrency 11",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker:dev": "docker compose --env-file .env -f _infrastructure/docker-compose.yml up",
    "db:push": "dotenv -- drizzle-kit push:pg"
  },
  "devDependencies": {
    "@mbsm/utils": "workspace:*",
    "@tsconfig/node18": "^18.2.2",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "0.20.14",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "sst": "^2.28.1",
    "turbo": "latest",
    "typescript": "^5.2.2"
  },
  "name": "mbsm",
  "volta": {
    "node": "18.17.1",
    "pnpm": "8.15.1"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}