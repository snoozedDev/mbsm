{
  "name": "mbsm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate:new": "dbmate new",
    "db:migrate:up": "dbmate up",
