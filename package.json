{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev --no-cache --continue --concurrency 11",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "dotenv -- drizzle-kit push:mysql"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "turbo": "latest"
  },
  "name": "mbsm",
  "packageManager": "yarn@3.6.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "volta": {
    "node": "18.18.0",
    "yarn": "3.6.3"
  },
  "engines": {
    "node": ">=18.18.0",
    "yarn": ">=3.6.3"
  }
}
