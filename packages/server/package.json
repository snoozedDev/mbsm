{
  "name": "@mbsm/server",
  "version": "0.0.0",
  "private": true,
  "description": "Server procedures for mbsm",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "pnpm build --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@mbsm/tsconfig": "workspace:*",
    "@mbsm/types": "workspace:*",
    "@mbsm/db-layer": "workspace:*",
    "@mbsm/utils": "workspace:*",
    "eslint": "8.51.0",
    "tsup": "8.0.1",
    "typescript": "5.2.2",
    "@trpc/server": "next"
  }
}