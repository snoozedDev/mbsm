{
  "name": "@mbsm/db-layer",
  "version": "0.0.0",
  "private": true,
  "description": "A database layer for mbsm",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "pnpm build -- --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@upstash/ratelimit": "1.0.0",
    "@upstash/redis": "1.28.3",
    "@vercel/kv": "1.0.1",
    "@vercel/postgres": "0.7.2",
    "drizzle-orm": "0.29.3",
    "postgres": "3.4.3"
  },
  "devDependencies": {
    "@mbsm/tsconfig": "*",
    "@mbsm/types": "*",
    "@mbsm/utils": "*",
    "drizzle-kit": "0.20.14",
    "eslint": "8.51.0",
    "tsup": "8.0.1",
    "typescript": "5.2.2"
  }
}